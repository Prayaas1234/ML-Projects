        "# Aggregate daily trading data\n",
        "daily_data = trades_df.groupby('date').agg({\n",
        "    'Closed PnL': 'sum',\n",
        "    'Size USD': 'sum',\n",
        "    'Execution Price': 'mean',\n",
        "    'Trade ID': 'count'\n",
        "}).reset_index()\n",
        "\n",
        "daily_data.rename(columns={\n",
        "    'Trade ID': 'trade_count',\n",
        "    'Size USD': 'volume'\n",
        "}, inplace=True)\n"
      ],
      "metadata": {
        "id": "PVKMF3ZruffP"
      },
      "execution_count": 11,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "final_df = pd.merge(\n",
        "    daily_data,\n",
        "    sentiment_df[['date', 'classification']],\n",
        "    on='date',\n",
        "    how='left'\n",
        ")\n"
      ],
      "metadata": {
        "id": "ejWI641zuslj"
      },
      "execution_count": 12,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Profit = 1, Loss = 0\n",
        "final_df['profit_label'] = final_df['Closed PnL'].apply(\n",
        "    lambda x: 1 if x > 0 else 0\n",
        ")\n"
      ],
      "metadata": {
        "id": "K8YTbCPEuuRo"
      },
      "execution_count": 13,
      "outputs": []
