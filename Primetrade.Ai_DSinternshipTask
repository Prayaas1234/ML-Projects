        "    'Fear': 1,\n",
        "    'Neutral': 2,\n",
        "    'Greed': 3,\n",
        "    'Extreme Greed': 4\n",
        "}\n",
        "\n",
        "final_df['sentiment_score'] = final_df['classification'].map(sentiment_map)\n"
      ],
      "metadata": {
        "id": "JW_L7w0Puv21"
